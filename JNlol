

-- MAIN


local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({JNHUB = "Title of the library", HidePremium = false, SaveConfig = false, ConfigFolder = "OrionUi", IntroText = ("JNHUB"), Name = ("JNHUB")})


--Anomic

-- TAB1
local Tab = Window:MakeTab({
	Name = "Anomic",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

-- Section1
local Section = Tab:AddSection({
	Name = "GUIs"
})


--Button1
Tab:AddButton({
	Name = "Gruizzy GUI",
	Callback = function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/GuizzyisbackV2LOL/AnomicGui.lua/main/LuaSource"))()
      		print("button pressed")
  	end    
}) 

--Button12
Tab:AddButton({
	Name = "ScriptX GUI",
	Callback = function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/scriptXHub/DiscordHiReplyBot-OLD-DOESNT-WORK-broken-/main/jjsongcatch"))()
      		print("button pressed")
  	end    
})




